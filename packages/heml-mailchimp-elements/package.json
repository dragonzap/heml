	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"dependencies": {
		"@dragonzap/heml-render": "file:../heml-render",
		"@dragonzap/heml-utils": "file:../heml-utils"
	},
	"devDependencies": {
		"typescript": "^4.0.5"
	}
}
