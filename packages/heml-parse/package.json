{
	"name": "@heml/parse",
	"version": "1.2.2-alpha.1",
	"description": "Parser for HEML",
	"keywords": [
		"heml"
	],
	"homepage": "https://heml.io",
	"bugs": "https://github.com/dragonzap/heml/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dragonzap/heml.git"
	},
	"author": "SparkPost <dev-relations@sparkpost.com> (https://sparkpost.com)",
	"files": [
		"build/"
	],
	"main": "build/index",
	"types": "build/index",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf ./build",
		"compile": "babel src --out-dir build --source-maps --extensions .ts,.tsx --delete-dir-on-start --config-file ../../babel.config.js --no-comments",
		"prepublishOnly": "npm run build"
	},
	"publishConfig": {
		"access": "public"
	},
	"dependencies": {
		"cheerio": "1.0.0-rc.3",
		"crypto-random-string": "^3.3.0",
		"html-tags": "^3.1.0",
		"juice": "^7.0.0",
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.22",
		"typescript": "^4.0.5"
	}
}
