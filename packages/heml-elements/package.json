{
	"name": "@dragonzap/elements",
	"version": "1.2.2-alpha.10",
	"description": "Core elements for HEML",
	"keywords": [
		"heml"
	],
	"homepage": "https://heml.io",
	"bugs": "https://github.com/dragonzap/heml/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dragonzap/heml.git"
	},
	"author": "Dragonzap <dragonzap.misi@gmail.com>",
	"files": [
		"build/"
	],
	"main": "build/index",
	"types": "build/index",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf ./build",
		"compile": "babel src --out-dir build --source-maps --extensions .ts,.tsx --delete-dir-on-start --config-file ../../babel.config.js --no-comments",
		"prepublishOnly": "npm run build"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"dependencies": {
		"@dragonzap/render": "file:../heml-render",
		"@dragonzap/styles": "file:../heml-styles",
		"@dragonzap/utils": "file:../heml-utils",
		"axios": "^0.21.0",
		"image-size": "^0.9.3",
		"is-absolute-url": "^3.0.3",
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"typescript": "^4.0.5"
	}
}
