{
	"name": "@heml/inline",
	"version": "1.2.2-alpha.9",
	"description": "CSS inliner for HEML",
	"keywords": [
		"heml"
	],
	"homepage": "https://heml.io",
	"bugs": "https://github.com/dragonzap/heml/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dragonzap/heml.git"
	},
	"author": "Dragonzap <dragonzap.misi@gmail.com>",
	"files": [
		"build/"
	],
	"main": "build/index",
	"types": "build/index",
	"scripts": {
		"build": "npm run clean && npm run compile",
		"clean": "rm -rf ./build",
		"compile": "babel src --out-dir build --source-maps --extensions .ts,.tsx --delete-dir-on-start --config-file ../../babel.config.js --no-comments",
		"prepublishOnly": "npm run build"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"dependencies": {
		"@heml/parse": "file:../heml-parse",
		"cheerio": "1.0.0-rc.3",
		"juice": "^7.0.0",
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.22",
		"typescript": "^4.0.5"
	}
}
