{
	"name": "@dragonzap/heml-render",
	"version": "1.2.2-alpha.11",
	"description": "Renderer for HEML",
	"keywords": [
		"heml"
	],
	"homepage": "https://heml.io",
	"bugs": "https://github.com/dragonzap/heml/issues",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/dragonzap/heml.git",
		"directory": "packages/heml-render"
	},
	"author": "Dragonzap <dragonzap.misi@gmail.com>",
	"files": [
		"build/"
	],
	"main": "build/index",
	"types": "build/index",
	"scripts": {
		"clean": "rm -rf ./dist",
		"build": "babel src --out-dir dist --source-maps --extensions .ts,.tsx --delete-dir-on-start --config-file ../../babel.config.js --no-comments",
		"prepublishOnly": "yarn run clean && yarn run build"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com/"
	},
	"dependencies": {
		"@dragonzap/heml-parse": "file:../heml-parse",
		"cheerio": "1.0.0-rc.3",
		"html-tags": "^3.1.0",
		"is-promise": "^4.0.0",
		"lodash": "^4.17.20"
	},
	"devDependencies": {
		"@types/cheerio": "^0.22.22",
		"typescript": "^4.0.5"
	}
}
